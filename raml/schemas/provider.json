{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "description": "Add new provider",
  "properties": {
    "providerName": {
      "type": "string"
    },
    "protocol": {
      "type": "string",
      "enum": [
        "single",
        "secure"
      ]
    },
    "singEndPoint": {
      "type": "string"
    },
    "jwt": {
      "type": "string"
    },
    "recoverPkEndPoint": {
      "type": "string"
    }
  },
  "required": [
    "providerName",
    "protocol",
    "singEndPoint"
  ]
}
