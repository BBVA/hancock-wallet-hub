{
  "name": "hancock-wallet-hub",
  "version": "1.0.0",
  "description": "Hancock Wallet Hub",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "concurrently \"npm run watch:ts\" \"sleep 3 && npm run serve:debug\"",
    "serve:prod": "node dist/index.js",
    "serve:debug": "nodemon --inspect=0.0.0.0 dist/index.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w --inlineSources --inlineSourceMap",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "docs": "rimraf documentation && mkdirp documentation && raml2html --theme raml2html-markdown-theme raml/api.raml > documentation/api.md",
    "plato": "plato -r -d complexity dist",
    "watch:plato": "plato -r -d complexity dist && open complexity/index.html"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.kickstartteam.es:7999/BH/kst-hancock-ms-wallet-hub.git"
  },
  "keywords": [
    "hancock",
    "wallet",
    "hub"
  ],
  "author": "Angel Manzano Alcaraz <angel.manzano.contractor@bbva.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "0.0.33",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mongodb": "2.2.18",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/uuid": "^3.4.3",
    "concurrently": "3.5.1",
    "jest": "^23.2.0",
    "mkdirp": "0.5.1",
    "raml2html": "6.4.2",
    "raml2html-markdown-theme": "1.0.2",
    "rimraf": "2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "5.8.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "1.29.2",
    "cors": "^2.8.4",
    "express": "4.16.2",
    "express-jsonschema": "^1.1.6",
    "jest-junit": "^5.1.0",
    "js-yaml": "3.10.0",
    "jsonwebtoken": "^7.1.9",
    "mongodb": "3.0.2",
    "node-forge": "0.7.5",
    "nodemon": "1.14.12",
    "plato": "^1.7.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.26",
    "web3-bzz": "1.0.0-beta.26",
    "web3-core": "1.0.0-beta.26",
    "web3-eth": "1.0.0-beta.26",
    "web3-eth-personal": "1.0.0-beta.26",
    "web3-net": "1.0.0-beta.26",
    "web3-shh": "1.0.0-beta.26",
    "web3-utils": "1.0.0-beta.26"
  }
}
